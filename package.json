{
  "name": "jiumen",
  "version": "0.0.1",
  "private": false,
  "description": "A strategic canvas for Web3 projects",
  "scripts": {
    "create-model:dev": "cross-env NODE_ENV=develop node --experimental-json-modules ./scripts/create-model.mjs",
    "publish-model:dev": "cross-env NODE_ENV=develop node --experimental-json-modules ./scripts/publish-model.mjs",
    "create-model": "cross-env NODE_ENV=production node --experimental-json-modules ./scripts/create-model.mjs",
    "publish-model": "cross-env NODE_ENV=production node --experimental-json-modules ./scripts/publish-model.mjs",
    "dev": "vite --mode develop",
    "build": "vite build",
    "build-tsc": "vue-tsc --noEmit && vite build",
    "serve": "vite preview --mode arlocal",
    "format": "prettier --write ./src",
    "lint": "eslint ./src --ext .vue,.js,.ts",
    "lint-fix": "eslint --fix ./src --ext .vue,.js,.ts",
    "prepare": "husky install",
    "test": "jest",
    "gh": "vite build && bash scripts/deploy-gh.sh GH=1"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:ZeroDAO/jiumen.git"
  },
  "keywords": [
    "Vite",
    "Vue3",
    "TypeScript",
    "Starter"
  ],
  "author": "DKLee",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ZeroDAO/jiumen/issues"
  },
  "homepage": "https://github.com/ZeroDAO/jiumen#readme",
  "lint-staged": {
    "*.{vue,js,ts}": "eslint --fix"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  },
  "dependencies": {
    "@ant-design/icons-vue": "^6.1.0",
    "@antv/x6": "^1.31.5",
    "@antv/x6-vue-shape": "^1.4.0",
    "@ceramicnetwork/3id-did-resolver": "^2.9.0",
    "@ckpack/vue-color": "^1.1.9",
    "@glazed/datamodel": "^0.3.1",
    "@glazed/did-datastore": "^0.3.1",
    "@glazed/tile-loader": "^0.2.1",
    "@self.id/web": "^0.5.0",
    "@walletconnect/web3-provider": "^1.8.0",
    "@web3-onboard/injected-wallets": "^2.4.0",
    "@web3-onboard/vue": "^2.4.3",
    "ant-design-vue": "3.2.3",
    "authereum": "^0.1.14",
    "axios": "^0.21.1",
    "dids": "^3.1.0",
    "eslint-define-config": "^1.15.0",
    "ethers": "^5.7.2",
    "fortmatic": "^2.4.0",
    "moment": "^2.29.4",
    "pinia": "2.0.13",
    "rimraf": "^4.1.2",
    "showdown": "^2.1.0",
    "vditor": "^3.8.14",
    "vite-plugin-commonjs-externals": "^0.1.1",
    "vite-plugin-windicss": "^1.8.4",
    "vue": "^3.2.6",
    "vue-composable": "^1.0.0-alpha.37",
    "vue-i18n": "^9.1.10",
    "vue-router": "^4.0.11",
    "vuex": "^4.0.2",
    "web3modal": "^1.9.11"
  },
  "devDependencies": {
    "@ceramicnetwork/common": "^2.2.1",
    "@ceramicnetwork/stream-tile": "^2.2.2",
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@glazed/devtools": "^0.1.6",
    "@glazed/did-datastore-model": "^0.2.0",
    "@glazed/types": "^0.2.0",
    "@types/jest": "^26.0.22",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "@vitejs/plugin-vue": "^1.6.0",
    "@vitejs/plugin-vue-jsx": "^2.0.0",
    "@vue/compiler-sfc": "^3.2.6",
    "@vue/test-utils": "^2.0.0-rc.4",
    "commitizen": "^4.2.3",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^6.3.0",
    "dotenv": "^16.0.1",
    "eslint": "^7.24.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.5",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.8.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "less": "^4.1.2",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "stylus": "^0.54.8",
    "ts-jest": "^26.5.4",
    "typescript": "^4.1.3",
    "vite": "3.0.0",
    "vite-plugin-element-plus": "0.0.12",
    "vue-jest": "^5.0.0-alpha.7",
    "vue-tsc": "^0.3.0"
  }
}
